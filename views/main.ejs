<div class="timeline">
    <h2>경매 진행 목록<h2>
    <table id="good-list">
        <tr>
            <th>상품명</th>
            <th>Image</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Entered Time</th>
        </tr>
        <% goods.forEach(good => { %>
            <tr>
                <td><%= good.name %> </td>
                <td><img src="/img/<%= good.img %>" alt=""></td>
                <td><%= good.price %> </td>
                <td class="time" data-start=<%= good.createdAt %>> 00:00:00</td>
                <td><a class="enter btn" href="/good/<%= good.id%>">입장</a></td>
            </tr>
        <% }) %>
    </table>

</div>